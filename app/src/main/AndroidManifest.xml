<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="njupt.b17070729.WaterAndFire">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="水深火热"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name="njupt.b17070729.WaterAndFire.Activity.MainActivity"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="njupt.b17070729.WaterAndFire.Activity.GameStartActivity" />
        <activity android:name="njupt.b17070729.WaterAndFire.Activity.config" />
    </application>

</manifest>